<!-- -*- mode: XML -*- -->
<launch>
  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find bobRob1)/controllers/srvss_bobcat_controllers.yaml" command="load"/>

  <!-- load the controllers -->
  <node name="bobRob1_controller_spawner" pkg="controller_manager" type="spawner" respawn="true"
	output="screen" ns="/bobRob1" args="joint_state_controller
					  front_right_wheel_velocity_controller
					  front_left_wheel_velocity_controller
					  back_right_wheel_velocity_controller
					  back_left_wheel_velocity_controller"/>


</launch>
